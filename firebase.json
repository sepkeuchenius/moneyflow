{
  "database": {
    "rules": "database.rules.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/callback",
        "destination": "/callback.html"
      },
      {
        "source": "/auth",
        "destination": "/auth.html"
      },
      {
        "source": "/settings",
        "destination": "/model.html"
      },
      {
        "source": "/annotate",
        "destination": "/annotate.html"
      },
      {
        "source": "/chart",
        "destination": "/chart.html"
      },
      {
        "source": "/model",
        "destination": "/model.html"
      },
      {
        "source": "/fastui",
        "function": {
          "functionId": "fastui_main",
          "region": "europe-west1"
        }
      },
      {
        "source": "/api/fastui",
        "function": {
          "functionId": "fastui_home",
          "region": "europe-west1"
        }
      }
    ]
  }
}